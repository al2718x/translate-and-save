{
  "browser_specific_settings": {
    "gecko": {
      "id": "translate-and-save@al2718x",
      "strict_min_version": "128.0"
    }
  },
  "manifest_version": 2,
  "name": "Translate and Save",
  "version": "0.0.1",
  "description": "Translate and Save",
  "icons": {
    "96": "icons/translate-and-save.png"
  },
